{
		"name": "@azbake/ingredient-azure-kubernetes-service",
		"description": "This ingredient will stand up an instance of the Azure Kubernetes Service (AKS/ACS)",
    "version": "0.0.1",
    "main": "dist/index.js",
		"typings": "src/index.ts",
		"author": "HCHB",
