		},
		"devDependencies": {
				"@azbake/core": "0.*",
				"@azure/ms-rest-nodeauth": "^0.9.2",
				"@types/node": "^10.14.14"
		},
		"dependencies": {
				"@azbake/arm-helper": "^0.1.68",
				"@azure/arm-containerservice": "^8.0.0"
		},
		"publishConfig": {
				"access": "public"
		}
}
