{
  "name": "@azbake/ingredient-azure-vm-extension",
  "description": "Ingredient for deploying Azure VM Extensions",
  "version": "0.0.1",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "author": "HCHB",
  "license": "MIT",
  "scripts": {
