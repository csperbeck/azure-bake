    "lib": "dist"
  },
  "peerDependencies": {
    "@azbake/core": "0.*",
    "@azure/ms-rest-nodeauth": "0.9.2"
  },
  "devDependencies": {
    "@azbake/core": "^0.1.64",
    "@azure/ms-rest-nodeauth": "0.9.2",
    "@types/node": "^10.12.18"
  },
  "dependencies": {
    "@azbake/arm-helper": "^0.1.64"
  },
  "publishConfig": {
    "access": "public"
  }
}
